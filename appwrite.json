{
    "projectId": "draconic-chatroom",
    "databases": [
        {
            "$id": "database",
            "name": "Database",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "messages",
            "databaseId": "database",
            "name": "messages",
            "documentSecurity": false,
            "documentSecurity": true,
            "attributes": [
                {
                    "$id": "text",
                    "key": "text",
                    "type": "string",
                    "size": 65535,
                    "required": true,
                    "signed": false,
                    "array": false,
                    "filters": []
                },
                {
                    "$id": "userId",
                    "key": "userId",
                    "type": "string",
                    "size": 255,
                    "required": true,
                    "signed": false,
                    "array": false,
                    "filters": []
                },
                {
                    "$id": "userName",
                    "key": "userName",
                    "type": "string",
                    "size": 255,
                    "signed": true,
                    "array": false,
                    "filters": []
                }
            ],
            "indexes": [],
            "enabled": true,
            "$permissions": [
                "read(\"any\")", "create(\"users\")"
            ]
        }
    ]
}
